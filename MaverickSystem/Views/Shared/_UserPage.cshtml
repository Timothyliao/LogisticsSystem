<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Maverick</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="这里是maverick物流信息系统." name="description" />
    <meta content="ljw" name="author" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="http://www.jq22.com/jquery/jquery-ui-1.11.0.css" />
    @Styles.Render("~/Content/themify")
    @Styles.Render("~/Content/flaticon")
    @Styles.Render("~/Content/swiper")
    @Styles.Render("~/Content/style")
    @Styles.Render("~/Content/jqueryUi")
    @Styles.Render("~/Content/maverickOrder")
    @Styles.Render("~/Content/pickuptime")
    @Styles.Render("~/Content/pick")
    <style type="text/css">
        .navBg {
            width: 100%;
            height: 100%;
            position: absolute;
            z-index: -1;
            background-color: rgba(52, 73, 94, 0.2);
            filter: blur(15px);
        }
    </style>
    @RenderSection("myStyle", required: false)
</head>
<body>
    <div class="page-wrapper">
        <div class="preloader">
            <div class="sk-folding-cube">
                <div class="sk-cube1 sk-cube"></div>
                <div class="sk-cube2 sk-cube"></div>
                <div class="sk-cube4 sk-cube"></div>
                <div class="sk-cube3 sk-cube"></div>
            </div>
        </div>

        <header id="header" class="site-header header-style-1">
            <div class="navBg"></div>
            <nav class="navigation navbar navbar-default">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="open-btn">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="/User/MainPage"><img src="~/UserAssets/img/icon/footer-logo.png"></a>
                    </div>
                    <div id="navbar" class="navbar-collapse collapse navbar-right navigation-holder">
                        <button class="close-navbar"><i class="ti-close"></i></button>
                        <ul class="nav navbar-nav">
                            <li class="menu-item">
                                <a href="@Url.Action("MainPage","User")">主页</a>
                            </li>
                            <li class="menu-item">
                                <a href="@Url.Action("TakeOrder","User")">寄件服务</a>
                            </li>
                            <li class="menu-item">
                                <a href="@Url.Action("CargoTrace","User")">运单追踪</a>
                            </li>
                            <li class="menu-item">
                                <a href="@Url.Action("Login","Staff")">员工登陆</a>
                            </li>
                        </ul>
                    </div>

                    <div class="separator"></div>

                </div>
            </nav>
        </header>

        @RenderBody()

    </div>

    <footer class="site-footer">
        <div class="container">
            <div class="row">
                <div class="col col-xs-12 text-center">
                    <p style="margin: 15px 0">Copyright &copy; 2020 LJW. All rights reserved.</p>
                </div>
            </div>
        </div>
    </footer>

    <script src="https://www.jq22.com/jquery/jquery-1.10.2.js"></script>

    @Scripts.Render("~/bundles/userbootstrapJS")
    @Scripts.Render("~/bundles/pluginCollectionJS")
    @Scripts.Render("~/bundles/scriptJS")
    @Scripts.Render("~/bundles/jqueryPlusUiJS")
    @Scripts.Render("~/bundles/jqueryUiSliderJS")
    @Scripts.Render("~/bundles/pickJS")
    @Scripts.Render("~/bundles/pickuptimeJS")
    @RenderSection("myScript", required: false)
</body>
</html>
